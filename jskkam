-- ç¿»è¯‘è®¾ç½®
createTab("ç¿»è¯‘è®¾ç½®", "ğŸŒ", function(contentParent)
    UIUtils.createParagraph(contentParent, "ç¿»è¯‘åŠŸèƒ½", "å·²åŠ è½½ " .. #Translations .. " æ¡è§„åˆ™\nä»…ç¿»è¯‘æŒ‡å®šå…³é”®è¯ï¼Œæ— å…¨å±€å†²çª")
   Â 
    UIUtils.createButton(contentParent, "è§¦å‘UIç¿»è¯‘", function()
        local function translateAllUI(parent)
            for _, child in ipairs(parent:GetChildren()) do
                if child:IsA("TextLabel") or child:IsA("TextButton") or child:IsA("TextBox") then
                    pcall(function()
                        if child.Text ~= "" then
                            child.Text = translateText(child.Text)
                        end
                        if child:IsA("TextBox") and child.PlaceholderText ~= "" then
                            child.PlaceholderText = translateText(child.PlaceholderText)
                        end
                    end)
                end
                translateAllUI(child)
            end
        end
        translateAllUI(MainGui)
        UIUtils.showNotification("ç¿»è¯‘å®Œæˆ", "å·²ç¿»è¯‘æ‰€æœ‰UIæ–‡æœ¬", 2)
    end)

    UIUtils.createButton(contentParent, "æµ‹è¯•ç¿»è¯‘å…³é”®è¯", function()
        local testText = "AxiosHub"
        local translated = translateText(testText)
        UIUtils.showNotification("ç¿»è¯‘æµ‹è¯•", testText .. " â†’ " .. translated, 3)
    end)
end)

-- ==================== 7. Deltaå¿«æ·é”®ä¸åˆå§‹åŒ– ====================
-- å¿«æ·é”®ï¼ˆDeltaä¼˜å…ˆç›‘å¬ï¼‰
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.RightControl then
        MainWindow.Visible = not MainWindow.Visible
        UIUtils.showNotification("UIæ§åˆ¶", MainWindow.Visible and "UIå·²æ˜¾ç¤º" or "UIå·²éšè—", 1)
    end
end)

-- åˆå§‹åŒ–ï¼ˆDeltaä¸“å±æç¤ºï¼‰
MainWindow.Parent = MainGui
if #Tabs > 0 then
    Tabs[1].Button:FireMouseButton1Click()
end

