local MainGui = Instance.new("ScreenGui")
MainGui.Name = "Delta_GUI_Translation"
MainGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling -- Deltaå±‚çº§å…¼å®¹
if isDelta and syn.protect_gui then
    syn.protect_gui(MainGui) -- ä¿æŠ¤UIä¸è¢«æ£€æµ‹
end
MainGui.Parent = UIParent

-- ä¸»çª—å£ï¼ˆæ”¯æŒæ‹–æ‹½ï¼‰
local MainWindow = Instance.new("Frame")
