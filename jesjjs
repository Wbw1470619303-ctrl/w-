    -- 8. å¿è€…ä¼ å¥‡Tabï¼ˆåŸç”Ÿç»„ä»¶ï¼‰
    local NinjaTab = MainWindow:Tab({Title="å¿è€…ä¼ å¥‡",Icon="shuriken"})
    NinjaTab:Section({Title="è‡ªåŠ¨åŠŸèƒ½"})
    NinjaTab:Toggle({
        Title = "è‡ªåŠ¨æŒ¥èˆ",
        Default = false,
        Callback = function(state)
            Global.autoswingEnabled = state
            if state then
                spawn(function()
                    while Global.autoswingEnabled do
                        task.wait()
                        if LocalPlayer:FindFirstChild("ninjaEvent") then
                            LocalPlayer.ninjaEvent:FireServer("swingKatana")
                        else
                            Global.autoswingEnabled = false
                            WindUI:Notify({Title="é”™è¯¯",Content="æœªæ‰¾åˆ°äº‹ä»¶",Duration=3})
                            break
                        end
                    end
                end)
                WindUI:Notify({Title="æˆåŠŸ",Content="è‡ªåŠ¨æŒ¥èˆå·²æ¿€æ´»",Duration=3})
            else
                WindUI:Notify({Title="å…³é—­",Content="è‡ªåŠ¨æŒ¥èˆå·²å…³é—­",Duration=3})
            end
        end
    })
    local autosellEnabled = false
    NinjaTab:Toggle({
        Title = "è‡ªåŠ¨å”®å–",
        Default = false,
        Callback = function(state)
            autosellEnabled = state
            if state then
                spawn(function()
                    while autosellEnabled do
                        task.wait(0.1)
                        local char = waitForCharacter()
                        if char then
                            local area = Workspace:FindFirstChild("sellAreaCircles")
                            if area and area:FindFirstChild("sellAreaCircle7") then
                                local inner = area.sellAreaCircle7:FindFirstChild("circleInner")
                                if inner then inner.CFrame = char.HumanoidRootPart.CFrame end
                            end
                        end
                    end
                end)
                WindUI:Notify({Title="æˆåŠŸ",Content="è‡ªåŠ¨å”®å–å·²æ¿€æ´»",Duration=3})
            else
                WindUI:Notify({Title="å…³é—­",Content="è‡ªåŠ¨å”®å–å·²å…³é—­",Duration=3})
            end
        end
    })

    -- 9. ä¿„äº¥ä¿„å·Tabï¼ˆåŸç”Ÿç»„ä»¶ï¼‰
    local OhioTab = MainWindow:Tab({Title="ä¿„äº¥ä¿„å·",Icon="map"})
    OhioTab:Section({Title="å¶è„šæœ¬åŠŸèƒ½"})
    OhioTab:Button({
        Title = "åŠ è½½å¶è„šæœ¬",
        Callback = function()
            if Global.ohioScript.loaded then
                WindUI:Notify({Title="æç¤º",Content="å¶è„šæœ¬å·²åŠ è½½",Duration=3})
                return
            end
            local success, scr = pcall(function()
                return game:HttpGet("https://raw.githubusercontent.com/roblox-ye/QQ515966991/refs/heads/main/YE-%20Scripts-OHIO.lua")
            end)
            if not success then
                WindUI:Notify({Title="å¤±è´¥",Content="å¶è„šæœ¬åŠ è½½å¤±è´¥",Duration=3})
                return
            end
            local exec = pcall(function() loadstring(scr)() end)
            if exec then
                Global.ohioScript.loaded = true
                WindUI:Notify({Title="æˆåŠŸ",Content="å¶è„šæœ¬å·²åŠ è½½",Duration=3})
            else
                WindUI:Notify({Title="å¤±è´¥",Content="å¶è„šæœ¬æ‰§è¡Œå¤±è´¥",Duration=3})
            end
        end
    })

    -- ä¿æ´»é€»è¾‘ï¼ˆä¿ç•™åŸå§‹é€»è¾‘ï¼‰
    spawn(function()
        while true do
            task.wait(3)
            if Global.MainWindow then
                local btn = CoreGui:FindFirstChild("WindUI_OpenButton")
                if not btn then btn = createOpenButton() end
                btn.Visible = true
                if not Global.rainbowAnimationConnection or not Global.rainbowAnimationConnection.Connected then
                    startRainbowAnimation(btn)
                end
            end
            LocalPlayer.CharacterAdded:Connect(function(newChar)
                task.wait(1)
                local hum = newChar:FindFirstChildOfClass("Humanoid")
                if hum then
                    hum.WalkSpeed = Global.currentWalkSpeed
                    hum.JumpPower = Global.currentJumpPower
                    if Global.godmodeEnabled then
                        hum.MaxHealth = math.huge
                        hum.Health = math.huge
                    end
                    if Global.autoHealEnabled then
                        Global.autoHealConnection = hum:GetPropertyChangedSignal("Health"):Connect(function()
                            if hum.Health < hum.MaxHealth then hum.Health = hum.MaxHealth end
                        end)
                    end
                    if Global.infiniteJumpEnabled then
                        Global.infiniteJumpConnection = hum.JumpRequest:Connect(function()
                            hum:ChangeState(Enum.HumanoidStateType.Jumping)
                            hum.CanJump = true
                        end)
                    end
                end
                if Global.noclipConnection then
                    for _,p in pairs(newChar:GetChildren()) do if p:IsA("BasePart") then p.CanCollide = false end end
                end
            end)
            if Global.wallhopConnection and not Global.wallhopConnection.Connected then Global.wallhopConnection = nil end
            if Global.flyConnection and not Global.flyConnection.Connected then Global.flyConnection = nil end
            for _,p in ipairs(Players:GetPlayers()) do
                if p.Name == "wbwhnb" and p ~= LocalPlayer then
                    local char = p.Character
                    if char and not char:FindFirstChild("CreatorTag") then task.wait(0.5) createCreatorTag(p) end
                end
            end
        end
    end)

    -- å¯åŠ¨æ£€æµ‹ï¼ˆä¿ç•™åŸå§‹é€»è¾‘ï¼‰
    spawn(function()
        task.wait(1)
        pcall(detectCreator)
        pcall(checkForWbwhnb)
    end)

    -- åŠ è½½æˆåŠŸé€šçŸ¥
    WindUI:Notify({Title="åŠ è½½æˆåŠŸ",Content="è„šæœ¬åŠŸèƒ½å·²å°±ç»ª",Duration=3})
end
