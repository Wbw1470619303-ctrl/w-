local Global = {
    WindUI = nil,
    MainWindow = nil,
    noclipConnection = nil,
    nightVisionEnabled = false,
    currentWalkSpeed = 16,
    currentJumpPower = 50,
    godmodeEnabled = false,
    autoswingEnabled = false,
    wallhopConnection = nil,
    rainbowAnimationConnection = nil,
    flyConnection = nil,
    creatorTagInstances = {},
    welcomeSent = false,
    ohioScript = { loaded = false },
    autoHealEnabled = false,
    autoHealConnection = nil,
    infiniteJumpEnabled = false,
    infiniteJumpConnection = nil,
    cameraLocked = false,
    originalCameraType = nil,
    currentMoveSensitivity = 1
}

-- åŠ è½½WindUIï¼ˆä¿ç•™åŸå§‹é€»è¾‘ï¼‰
local success, WindUI = pcall(function()
    local framework = game:HttpGet("https://raw.githubusercontent.com/Footagesus/WindUI/main/dist/main.lua")
    return loadstring(framework)()
end)
if not success then
    local notify = Instance.new("ScreenGui", game.Players.LocalPlayer.PlayerGui)
    local lbl = Instance.new("TextLabel", notify)
    lbl.Size = UDim2.fromOffset(300,80)
    lbl.Position = UDim2.fromScale(0.5,0.1)
    lbl.AnchorPoint = Vector2.new(0.5,0.5)
    lbl.BackgroundColor3 = Color3.new(0,0,0)
    lbl.TextColor3 = Color3.new(1,0,0)
    lbl.Text = "WindUIåŠ è½½å¤±è´¥\næ£€æŸ¥ç½‘ç»œ/æ›´æ–°Delta"
    lbl.TextWrapped = true
    task.wait(5)
    notify:Destroy()
    return
end
Global.WindUI = WindUI

-- åŸºç¡€æœåŠ¡ï¼ˆä¿ç•™åŸå§‹é€»è¾‘ï¼‰
local LocalPlayer = game.Players.LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")
local RunService = game:GetService("RunService")
local Lighting = game:GetService("Lighting")
local CoreGui = game:GetService("CoreGui")
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local TextChatService
local successTextChat = pcall(function() TextChatService = game:GetService("TextChatService") end)
local Confirmed = false

-- è§’è‰²åŠ è½½ã€å½©è™¹è‰²ã€æ ‡ç­¾åˆ›å»ºç­‰å·¥å…·å‡½æ•°ï¼ˆå®Œå…¨ä¿ç•™åŸå§‹é€»è¾‘ï¼‰
local function waitForCharacter()
    local timeout = 0
    while not LocalPlayer.Character or not LocalPlayer.Character:FindFirstChild("HumanoidRootPart") do
        task.wait(0.1)
        timeout += 0.1
        if timeout >= 10 then return nil end
    end
    return LocalPlayer.Character
end
local function getRainbowColor(hueOffset)
    local hue = (tick() * 0.2 + hueOffset) % 1
    return Color3.fromHSV(hue, 0.8, 0.9)
end
local function startRainbowAnimation(openButton)
    if Global.rainbowAnimationConnection then Global.rainbowAnimationConnection:Disconnect() end
    Global.rainbowAnimationConnection = RunService.RenderStepped:Connect(function()
        if openButton and openButton.Parent then
            openButton.Color = ColorSequence.new(getRainbowColor(0), getRainbowColor(0.1))
        else
            Global.rainbowAnimationConnection:Disconnect()
            Global.rainbowAnimationConnection = nil
        end
    end)
end
local function createCreatorTag(player)
    if not player or player == LocalPlayer or not player.Character then return end
    local hrp = player.Character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    if Global.creatorTagInstances[player.UserId] then
        pcall(function() Global.creatorTagInstances[player.UserId]:Destroy() end)
        Global.creatorTagInstances[player.UserId] = nil
    end
    local success, bbg = pcall(function()
        local bg = Instance.new("BillboardGui")
        bg.Name = "CreatorTag"
        bg.Adornee = hrp
        bg.Size = UDim2.fromOffset(300,70)
        bg.AlwaysOnTop = true
        bg.Parent = Workspace
        bg.MaxDistance = 600
        bg.ZIndexBehavior = Enum.ZIndexBehavior.Global
        local stroke = Instance.new("UIStroke", bg)
        stroke.Thickness = 3
        stroke.Color = Color3.new(0,1,1)
        local frame = Instance.new("Frame", bg)
        frame.Size = UDim2.fromScale(1,1)
        frame.BackgroundTransparency = 0.1
        frame.BackgroundColor3 = Color3.new(0,0,0)
        Instance.new("UICorner", frame).CornerRadius = UDim.new(0,4)
        local lbl = Instance.new("TextLabel", frame)
        lbl.Size = UDim2.fromScale(1,1)
        lbl.BackgroundTransparency = 1
        lbl.Text = "[W] è¶…çº§è„šæœ¬åˆ¶ä½œè€… wbwhnb"
        lbl.TextColor3 = Color3.new(1,0.8,0)
        lbl.TextSize = 24
        lbl.Font = Enum.Font.RobotoCondensed
        lbl.TextWrapped = true
        lbl.TextStrokeColor3 = Color3.new(0,0,0)
        lbl.TextStrokeTransparency = 0
        local bar = Instance.new("Frame", frame)
        bar.Size = UDim2.fromScale(1,0.15)
        bar.Position = UDim2.fromScale(0,0.85)
        bar.BackgroundColor3 = Color3.new(1,0,1)
        bar.BackgroundTransparency = 0.4
        Instance.new("UICorner", bar).CornerRadius = UDim.new(0,2)
        return bg
    end)
    if success and bbg then
        Global.creatorTagInstances[player.UserId] = bbg
    else
        WindUI:Notify({Title="æ ‡ç­¾æç¤º",Content="ä½œè€…æ ‡è¯†åˆ›å»ºå®Œæˆ",Duration=2})
    end
end
local function SendChatMessage(message)
    pcall(function()
        if successTextChat and TextChatService then
            if TextChatService.ChatVersion == Enum.ChatVersion.TextChatService then
                local ch = TextChatService.TextChannels:FindFirstChild("RBXGeneral")
                if ch then ch:SendAsync(message) end
            else
                local ev = ReplicatedStorage:FindFirstChild("DefaultChatSystemChatEvents")
                local req = ev and ev:FindFirstChild("SayMessageRequest")
                if req then req:FireServer(message, "All") end
            end
        else
            local ev = ReplicatedStorage:FindFirstChild("DefaultChatSystemChatEvents")
            local req = ev and ev:FindFirstChild("SayMessageRequest")
            if req then req:FireServer(message, "All") end
        end
    end)
end
local function checkForWbwhnb()
    for _,p in ipairs(Players:GetPlayers()) do
        if p.Name == "wbwhnb" then
            SendChatMessage("æ£€æµ‹åˆ°è¶…çº§è„šæœ¬åˆ¶ä½œè€…ï¼ğŸ‰æ¬¢è¿wbwhnbåŠ å…¥ï¼ğŸ‰")
            break
        end
    end
end
local function sendWelcomeMessage(player)
    if Global.welcomeSent or not player:IsDescendantOf(Players) then return end
    Global.welcomeSent = true
    pcall(function()
        local gui = Instance.new("ScreenGui", LocalPlayer.PlayerGui)
        gui.Name = "WelcomeNotify"
        local lbl = Instance.new("TextLabel", gui)
        lbl.Size = UDim2.fromOffset(400,60)
        lbl.Position = UDim2.fromScale(0.5,0.05)
        lbl.AnchorPoint = Vector2.new(0.5,0)
        lbl.BackgroundColor3 = Color3.new(0,0,0)
        lbl.BackgroundTransparency = 0.3
        lbl.TextColor3 = Color3.new(1,1,0)
        lbl.Text = "æ¬¢è¿wè¶…çº§è„šæœ¬åˆ¶ä½œè€… @"..player.Name.." åŠ å…¥ï¼ğŸ‰"
        lbl.TextSize = 18
        lbl.TextWrapped = true
        task.wait(12)
        gui:Destroy()
    end)
    spawn(function()
        task.wait(1.2)
        SendChatMessage("æ¬¢è¿Wè¶…çº§è„šæœ¬åˆ¶ä½œè€…wbwhnbåŠ å…¥ğŸ‰")
        WindUI:Notify({Title="æˆåŠŸ",Content="æ¬¢è¿ä¿¡æ¯å·²å‘é€",Duration=2})
    end)
end
local function detectCreator()
    local function checkAuthor()
        for _,p in ipairs(Players:GetPlayers()) do
            if p.Name == "wbwhnb" and p ~= LocalPlayer then
                if p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
                    createCreatorTag(p)
                    sendWelcomeMessage(p)
                    return true
                end
            end
        end
        return false
    end
    if not checkAuthor() then
        spawn(function() while true do task.wait(3) if checkAuthor() then break end end end)
    end
    Players.PlayerAdded:Connect(function(p)
        if p.Name == "wbwhnb" and p ~= LocalPlayer then
            task.wait(1.5)
            createCreatorTag(p)
            sendWelcomeMessage(p)
        end
    end)
    Players.PlayerAdded:Connect(function(p)
        if p.Name == "wbwhnb" and p ~= LocalPlayer then
            p.CharacterAdded:Connect(function() task.wait(1.2) createCreatorTag(p) end)
        end
    end)
end
Players.PlayerAdded:Connect(function(p)
    if p.Name == "wbwhnb" then task.wait(1) SendChatMessage("ğŸ‰æ¬¢è¿wbwhnbåŠ å…¥ï¼ğŸ‰") end
end)

-- åˆå§‹å¼¹çª—ï¼ˆä¿ç•™åŸå§‹é€»è¾‘ï¼‰
WindUI:Popup({
    Title = "w è¶…çº§è„šæœ¬",
    IconThemed = true,
    Content = "æ¬¢è¿ä½¿ç”¨wè¶…çº§è„šæœ¬\nå½“å‰ç”¨æˆ·: "..LocalPlayer.Name,
    Buttons = {
        {Title="å–æ¶ˆ",Callback=function() end},
        {Title="åŠ è½½è„šæœ¬",Callback=function() Confirmed=true; createMainUI() end}
    }
})

-- ä¸»UIåˆ›å»ºï¼ˆæ ¸å¿ƒä¿®å¤ï¼šç”¨WindUIåŸç”Ÿç»„ä»¶+æœ€ç®€å¸ƒå±€ï¼‰
function createMainUI()
    -- 1. åˆ›å»ºä¸»çª—å£ï¼ˆæ ‡å‡†å†™æ³•ï¼‰
    local MainWindow = WindUI:CreateWindow({
        Title = "w è¶…çº§è„šæœ¬",
        Icon = "crown",
        Author = "wbwhnb (QQ:2318995920)",
        Folder = "wSuperScript",
        Size = UDim2.fromOffset(600,400),
        Theme = "Dark",
        SideBarWidth = 220,
        OnClose = function() MainWindow:Hide() end,
        OnMinimize = function()
            MainWindow:Hide()
            local btn = CoreGui:FindFirstChild("WindUI_OpenButton")
            if btn then btn.Visible = true end
        end
    })
    Global.MainWindow = MainWindow

    -- 2. åˆ›å»ºå°çª—æŒ‰é’®ï¼ˆæ ‡å‡†å†™æ³•ï¼‰
    local function createOpenButton()
        local btn = MainWindow:EditOpenButton({
            Title = "w è¶…çº§è„šæœ¬",
            Icon = "crown",
            CornerRadius = UDim.new(0,10),
            StrokeThickness = 2,
            Color = ColorSequence.new(getRainbowColor(0), getRainbowColor(0.1)),
            Draggable = true,
            Parent = CoreGui,
            ZIndex = 9999,
            Size = UDim2.fromOffset(120,40)
        })
        startRainbowAnimation(btn)
        return btn
    end
    local openButton = createOpenButton()

    -- 3. ä½œè€…ä¿¡æ¯Tabï¼ˆåŸç”Ÿç»„ä»¶ï¼‰
    local AuthorTab = MainWindow:Tab({Title="ä½œè€…ä¿¡æ¯",Icon="user"})
    AuthorTab:Section({Title="è„šæœ¬ä½œè€…è¯¦æƒ…"})
    AuthorTab:Paragraph({Text="ä½œè€…: wbwhnb"})
    AuthorTab:Paragraph({Text="ä½œè€…QQ: 2318995920"})
    AuthorTab:Paragraph({Text="è„šæœ¬æ€§è´¨: å…è´¹å¼€æº"})
    AuthorTab:Button({
        Title = "å¤åˆ¶ä½œè€…QQ",
        Callback = function()
            setclipboard("2318995920")
            WindUI:Notify({Title="æç¤º",Content="QQå·²å¤åˆ¶",Duration=3})
        end
    })

    -- 4. é€šç”¨åŠŸèƒ½Tabï¼ˆæ ¸å¿ƒä¿®å¤ï¼šç”¨WindUI.Rowæ’ç‰ˆæŒ‰é’®ï¼‰
    local GeneralTab = MainWindow:Tab({Title="é€šç”¨åŠŸèƒ½",Icon="settings"})
    GeneralTab:Section({Title="åŸºç¡€å±æ€§è°ƒæ•´"})

    -- ç§»åŠ¨é€Ÿåº¦ï¼ˆRow+æŒ‰é’®ï¼‰
    GeneralTab:Paragraph({Text="ç§»åŠ¨é€Ÿåº¦ï¼ˆå½“å‰: "..Global.currentWalkSpeed.."ï¼‰"})
    local SpeedRow = GeneralTab:Row({Size=UDim2.fromScale(1,0.1)})
    SpeedRow:Button({
        Title = "-",
        Size = UDim2.fromScale(0.15,1),
        Color = Color3.new(1,0,0),
        Callback = function()
            if Global.currentWalkSpeed > 16 then
                Global.currentWalkSpeed -=5
                local char = waitForCharacter()
                if char then char:FindFirstChildOfClass("Humanoid").WalkSpeed = Global.currentWalkSpeed end
                -- æ›´æ–°æ˜¾ç¤º
                for _,c in ipairs(GeneralTab.Content:GetChildren()) do
                    if c:IsA("WindUI.Paragraph") and c.Text:find("ç§»åŠ¨é€Ÿåº¦") then
                        c.Text = "ç§»åŠ¨é€Ÿåº¦ï¼ˆå½“å‰: "..Global.currentWalkSpeed.."ï¼‰"
                    end
                end
            end
        end
    })
    SpeedRow:Button({
        Title = "+",
        Size = UDim2.fromScale(0.15,1),
        Color = Color3.new(0,1,0),
        Callback = function()
            if Global.currentWalkSpeed < 200 then
                Global.currentWalkSpeed +=5
                local char = waitForCharacter()
                if char then char:FindFirstChildOfClass("Humanoid").WalkSpeed = Global.currentWalkSpeed end
                -- æ›´æ–°æ˜¾ç¤º
                for _,c in ipairs(GeneralTab.Content:GetChildren()) do
                    if c:IsA("WindUI.Paragraph") and c.Text:find("ç§»åŠ¨é€Ÿåº¦") then
                        c.Text = "ç§»åŠ¨é€Ÿåº¦ï¼ˆå½“å‰: "..Global.currentWalkSpeed.."ï¼‰"
                    end
                end
            end
        end
    })

    -- è·³è·ƒé«˜åº¦ï¼ˆRow+æŒ‰é’®ï¼‰
    GeneralTab:Paragraph({Text="è·³è·ƒé«˜åº¦ï¼ˆå½“å‰: "..Global.currentJumpPower.."ï¼‰"})
    local JumpRow = GeneralTab:Row({Size=UDim2.fromScale(1,0.1)})
    JumpRow:Button({
        Title = "-",
        Size = UDim2.fromScale(0.15,1),
        Color = Color3.new(1,0,0),
        Callback = function()
            if Global.currentJumpPower > 50 then
                Global.currentJumpPower -=5
                local char = waitForCharacter()
                if char then char:FindFirstChildOfClass("Humanoid").JumpPower = Global.currentJumpPower end
                -- æ›´æ–°æ˜¾ç¤º
                for _,c in ipairs(GeneralTab.Content:GetChildren()) do
                    if c:IsA("WindUI.Paragraph") and c.Text:find("è·³è·ƒé«˜åº¦") then
                        c.Text = "è·³è·ƒé«˜åº¦ï¼ˆå½“å‰: "..Global.currentJumpPower.."ï¼‰"
                    end
                end
            end
        end
    })
    JumpRow:Button({
        Title = "+",
        Size = UDim2.fromScale(0.15,1),
        Color = Color3.new(0,1,0),
        Callback = function()
            if Global.currentJumpPower < 200 then
                Global.currentJumpPower +=5
                local char = waitForCharacter()
                if char then char:FindFirstChildOfClass("Humanoid").JumpPower = Global.currentJumpPower end
                -- æ›´æ–°æ˜¾ç¤º
                for _,c in ipairs(GeneralTab.Content:GetChildren()) do
                    if c:IsA("WindUI.Paragraph") and c.Text:find("è·³è·ƒé«˜åº¦") then
                        c.Text = "è·³è·ƒé«˜åº¦ï¼ˆå½“å‰: "..Global.currentJumpPower.."ï¼‰"
                    end
                end
            end
        end
    })

    -- ç§»åŠ¨çµæ•åº¦ï¼ˆRow+æŒ‰é’®ï¼‰
    GeneralTab:Paragraph({Text="ç§»åŠ¨çµæ•åº¦ï¼ˆå½“å‰: "..Global.currentMoveSensitivity.."ï¼‰"})
    local SensRow = GeneralTab:Row({Size=UDim2.fromScale(1,0.1)})
    SensRow:Button({
        Title = "-",
        Size = UDim2.fromScale(0.15,1),
        Color = Color3.new(1,0,0),
        Callback = function()
            if Global.currentMoveSensitivity > 0.5 then
                Global.currentMoveSensitivity = math.round((Global.currentMoveSensitivity - 0.1)*10)/10
                pcall(function() LocalPlayer:GetMouse().Sensitivity = Global.currentMoveSensitivity end)
                -- æ›´æ–°æ˜¾ç¤º
                for _,c in ipairs(GeneralTab.Content:GetChildren()) do
                    if c:IsA("WindUI.Paragraph") and c.Text:find("ç§»åŠ¨çµæ•åº¦") then
                        c.Text = "ç§»åŠ¨çµæ•åº¦ï¼ˆå½“å‰: "..Global.currentMoveSensitivity.."ï¼‰"
                    end
                end
            end
        end
    })
    SensRow:Button({
        Title = "+",
        Size = UDim2.fromScale(0.15,1),
        Color = Color3.new(0,1,0),
        Callback = function()
            if Global.currentMoveSensitivity < 5 then
                Global.currentMoveSensitivity = math.round((Global.currentMoveSensitivity + 0.1)*10)/10
                pcall(function() LocalPlayer:GetMouse().Sensitivity = Global.currentMoveSensitivity end)
                -- æ›´æ–°æ˜¾ç¤º
                for _,c in ipairs(GeneralTab.Content:GetChildren()) do
                    if c:IsA("WindUI.Paragraph") and c.Text:find("ç§»åŠ¨çµæ•åº¦") then
                        c.Text = "ç§»åŠ¨çµæ•åº¦ï¼ˆå½“å‰: "..Global.currentMoveSensitivity.."ï¼‰"
                    end
                end
            end
        end
    })

    -- é€šç”¨åŠŸèƒ½å…¶ä»–ç»„ä»¶ï¼ˆåŸç”Ÿå†™æ³•ï¼‰
    GeneralTab:Toggle({
        Title = "å¤œè§†æ¨¡å¼",
        Default = Global.nightVisionEnabled,
        Callback = function(state)
            Global.nightVisionEnabled = state
            Lighting.Ambient = state and Color3.new(0.8,0.8,0.8) or Color3.new(0.1,0.1,0.1)
            Lighting.OutdoorAmbient = state and Color3.new(0.6,0.6,0.6) or Color3.new(0.2,0.2,0.2)
        end
    })
    GeneralTab:Toggle({
        Title = "ç©¿å¢™æ¨¡å¼",
        Default = false,
        Callback = function(state)
            if state then
                Global.noclipConnection = RunService.Stepped:Connect(function()
                    local char = LocalPlayer.Character
                    if char then for _,p in pairs(char:GetChildren()) do if p:IsA("BasePart") then p.CanCollide = false end end end
                end)
            else
                if Global.noclipConnection then Global.noclipConnection:Disconnect() Global.noclipConnection = nil end
                local char = LocalPlayer.Character
                if char then for _,p in pairs(char:GetChildren()) do if p:IsA("BasePart") then p.CanCollide = true end end end
            end
        end
    })
    GeneralTab:Button({
        Title = "æ— æ•Œæ¨¡å¼",
        Callback = function()
            Global.godmodeEnabled = not Global.godmodeEnabled
            local char = waitForCharacter()
            if char then
                local hum = char:FindFirstChildOfClass("Humanoid")
                if hum then
                    if Global.godmodeEnabled then
                        hum.MaxHealth = math.huge
                        hum.Health = math.huge
                        hum:GetPropertyChangedSignal("Health"):Connect(function() if hum.Health < math.huge then hum.Health = math.huge end end)
                        WindUI:Notify({Title="æˆåŠŸ",Content="æ— æ•Œå·²æ¿€æ´»",Duration=3})
                    else
                        hum.MaxHealth = 100
                        hum.Health = 100
                        WindUI:Notify({Title="å…³é—­",Content="æ— æ•Œå·²å…³é—­",Duration=3})
                    end
                end
            else
                WindUI:Notify({Title="é”™è¯¯",Content="è§’è‰²æœªåŠ è½½",Duration=2})
            end
        end
    })
    GeneralTab:Toggle({
        Title = "è‡ªåŠ¨å›è¡€",
        Default = Global.autoHealEnabled,
        Callback = function(state)
            Global.autoHealEnabled = state
            if Global.autoHealConnection then Global.autoHealConnection:Disconnect() Global.autoHealConnection = nil end
            if state then
                local char = waitForCharacter()
                if char then
                    local hum = char:FindFirstChildOfClass("Humanoid")
                    if hum then
                        Global.autoHealConnection = hum:GetPropertyChangedSignal("Health"):Connect(function()
                            if hum.Health < hum.MaxHealth then hum.Health = hum.MaxHealth end
                        end)
                        WindUI:Notify({Title="æˆåŠŸ",Content="è‡ªåŠ¨å›è¡€å·²æ¿€æ´»",Duration=3})
                    end
                end
            else
                WindUI:Notify({Title="å…³é—­",Content="è‡ªåŠ¨å›è¡€å·²å…³é—­",Duration=3})
            end
        end
    })
    GeneralTab:Toggle({
        Title = "æ— é™è·³è·ƒ",
        Default = Global.infiniteJumpEnabled,
        Callback = function(state)
            Global.infiniteJumpEnabled = state
            if Global.infiniteJumpConnection then Global.infiniteJumpConnection:Disconnect() Global.infiniteJumpConnection = nil end
            if state then
                local char = waitForCharacter()
                if char then
                    local hum = char:FindFirstChildOfClass("Humanoid")
                    if hum then
                        Global.infiniteJumpConnection = hum.JumpRequest:Connect(function()
                            hum:ChangeState(Enum.HumanoidStateType.Jumping)
                            hum.CanJump = true
                        end)
                        WindUI:Notify({Title="æˆåŠŸ",Content="æ— é™è·³è·ƒå·²æ¿€æ´»",Duration=3})
                    end
                end
            else
                WindUI:Notify({Title="å…³é—­",Content="æ— é™è·³è·ƒå·²å…³é—­",Duration=3})
            end
        end
    })
    GeneralTab:Toggle({
        Title = "è§†è§’é”å®š",
        Default = Global.cameraLocked,
        Callback = function(state)
            Global.cameraLocked = state
            local cam = Workspace.CurrentCamera
            if not cam then return end
            if state then
                Global.originalCameraType = cam.CameraType
                cam.CameraType = Enum.CameraType.Fixed
                WindUI:Notify({Title="æˆåŠŸ",Content="è§†è§’å·²é”å®š",Duration=3})
            else
                if Global.originalCameraType then cam.CameraType = Global.originalCameraType end
                WindUI:Notify({Title="å…³é—­",Content="è§†è§’å·²è§£é”",Duration=3})
            end
        end
    })
    GeneralTab:Button({
        Title = "å¿«é€Ÿå›è¡€",
        Callback = function()
            local char = waitForCharacter()
            if char then
                local hum = char:FindFirstChildOfClass("Humanoid")
                if hum then hum.Health = hum.MaxHealth end
                WindUI:Notify({Title="æˆåŠŸ",Content="è¡€é‡å·²å›æ»¡",Duration=2})
            else
                WindUI:Notify({Title="é”™è¯¯",Content="è§’è‰²æœªåŠ è½½",Duration=2})
            end
        end
    })
    GeneralTab:Button({
        Title = "ç‚¹å‡»ä¼ é€",
        Callback = function()
            local tool = Instance.new("Tool")
            tool.RequiresHandle = false
            tool.Name = "[FE] ç‚¹å‡»ä¼ é€"
            tool.Activated:Connect(function()
                local mouse = LocalPlayer:GetMouse()
                local char = waitForCharacter()
                if mouse.Hit and char then
                    char.HumanoidRootPart.CFrame = CFrame.new(mouse.Hit.Position + Vector3.new(0,2.5,0))
                end
            end)
            tool.Parent = LocalPlayer.Backpack
            WindUI:Notify({Title="è·å–",Content="ä¼ é€å·¥å…·å·²æ”¾å…¥èƒŒåŒ…",Duration=2})
        end
    })
    GeneralTab:Button({
        Title = "è·³å¢™æ¨¡å¼",
        Callback = function()
            if Global.wallhopConnection then
                Global.wallhopConnection:Disconnect()
                Global.wallhopConnection = nil
