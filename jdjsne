            if openButton then openButton.Visible = true end
        end,
    })
    Global.MainWindow = MainWindow

    local function createOpenButton()
        local openButton = MainWindow:EditOpenButton({
            Title = "w è¶…çº§è„šæœ¬",
            Icon = "crown",
            CornerRadius = UDim.new(0, 10),
