                    if humanoid then humanoid.WalkSpeed = 100 end
                end
                WindUI:Notify({ Title = "é€Ÿåº¦è®¾ç½®", Content = "ç§»åŠ¨é€Ÿåº¦å·²è®¾ç½®ä¸º: 100", Duration = 2 })
            elseif Global.currentWalkSpeed == 100 then
                Global.currentWalkSpeed = 200
