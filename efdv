-- æ¡†æ¶åŠ è½½ - ä½¿ç”¨æœ¬åœ°åŒ–ç‰ˆæœ¬é¿å…ç½‘ç»œè¯·æ±‚
local success, WindUI = pcall(function()
    -- è¿™é‡Œæ˜¯ç®€åŒ–çš„æœ¬åœ°WindUIå®ç°
    local LocalWindUI = {}
   Â 
    function LocalWindUI:CreateWindow(options)
        local window = {
            Hide = function() end,
            Tab = function(tabOptions)
                local tab = {
                    Section = function(sectionOptions) return {} end,
                    Paragraph = function(paragraphOptions) end,
                    Button = function(buttonOptions)Â 
                        if buttonOptions.Callback then
                            buttonOptions.Callback()
                        end
                    end,
                    Toggle = function(toggleOptions)
                        if toggleOptions.Callback then
                            toggleOptions.Callback(toggleOptions.Default or false)
                        end
                    end,
                    Slider = function(sliderOptions)
                        if sliderOptions.Callback then
                            sliderOptions.Callback(sliderOptions.Default or sliderOptions.Min or 0)
                        end
                    end
                }
                return tab
            end,
            EditOpenButton = function(buttonOptions)
                local openButton = Instance.new("TextButton")
                openButton.Name = "WindUI_OpenButton"
                openButton.Size = buttonOptions.Size or UDim2.new(0, 120, 0, 40)
                openButton.Position = UDim2.new(0, 10, 0, 10)
                openButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
                openButton.TextColor3 = Color3.new(1, 1, 1)
                openButton.Text = buttonOptions.Title or "Open"
                openButton.Parent = buttonOptions.Parent
                return openButton
            end
        }
        return window
    end
   Â 
    function LocalWindUI:Popup(options)
        if options.Buttons then
            for _, button in ipairs(options.Buttons) do
                if button.Title == "åŠ è½½è„šæœ¬" and button.Callback then
                    button.Callback()
                end
            end
        end
    end
   Â 
    function LocalWindUI:Notify(options)
        print("Notification: " .. (options.Title or "") .. " - " .. (options.Content or ""))
    end
   Â 
    return LocalWindUI
end)

if not success then
