                Global.currentMoveSensitivity = 3
                pcall(function()
                    local mouse = LocalPlayer:GetMouse()
                    mouse.Sensitivity = 3
                end)
                WindUI:Notify({ Title = "çµæ•åº¦è®¾ç½®", Content = "ç§»åŠ¨çµæ•åº¦å·²è®¾ç½®ä¸º: 3", Duration = 2 })
            elseif Global.currentMoveSensitivity == 3 then
                Global.currentMoveSensitivity = 5
                pcall(function()
                    local mouse = LocalPlayer:GetMouse()
                    mouse.Sensitivity = 5
                end)
                WindUI:Notify({ Title = "çµæ•åº¦è®¾ç½®", Content = "ç§»åŠ¨çµæ•åº¦å·²è®¾ç½®ä¸º: 5", Duration = 2 })
            else
