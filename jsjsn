    tabContainer.Size = UDim2.fromScale(0.25, 0.92)
    tabContainer.Position = UDim2.fromScale(0, 0.08)
    tabContainer.BackgroundColor3 = Color3.new(0.08,0.08,0.08)
    local tabTitles = {"ä½œè€…ä¿¡æ¯", "é€šç”¨åŠŸèƒ½", "å¨±ä¹åŠŸèƒ½", "DOORS", "åŠ›é‡ä¼ å¥‡", "å¿è€…ä¼ å¥‡", "ä¿„äº¥ä¿„å·"}
    local tabIcons = {"ğŸ‘¤", "âš™ï¸", "ğŸ®", "ğŸšª", "ğŸ’ª", "ğŸ—¡ï¸", "ğŸŒ"}
    local yOffset = 0.02
    for i, title in ipairs(tabTitles) do
        local btn = createButton(tabContainer, tabIcons[i].." "..title, UDim2.fromScale(0.05, yOffset), UDim2.fromScale(0.9, 0.12), Color3.new(0.2,0.2,0.2), function()
            -- åˆ‡æ¢æ ‡ç­¾
            if Global.CurrentTab then Global.CurrentTab.Visible = false end
            Global.CurrentTab = Global[title.."Content"]
            Global.CurrentTab.Visible = true
            -- é«˜äº®å½“å‰æŒ‰é’®
            for _, b in pairs(Global.TabButtons) do b.BackgroundColor3 = Color3.new(0.2,0.2,0.2) end
