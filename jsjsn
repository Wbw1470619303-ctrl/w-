    local section2 = createSection(Global["é€šç”¨åŠŸèƒ½Content"], "åŸºç¡€åŠŸèƒ½", UDim2.fromScale(0.05, 0.35), UDim2.fromScale(0.9, 0.25))
    createToggle(section2, "å¤œè§†æ¨¡å¼", UDim2.fromScale(0.05, 0.2), UDim2.fromScale(0.4, 0.2), Global.nightVisionEnabled, function(state)
        Global.nightVisionEnabled = state
        Lighting.Ambient = state and Color3.new(0.8,0.8,0.8) or Color3.new(0.1,0.1,0.1)
        Lighting.OutdoorAmbient = state and Color3.new(0.6,0.6,0.6) or Color3.new(0.2,0.2,0.2)
    end)
