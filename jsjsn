-- åˆå§‹å¼¹çª—ï¼ˆåŸç”ŸGUIï¼Œé¿å…WindUIå¼¹çª—é—®é¢˜ï¼‰
local function showStartupPopup()
    local popupGui = Instance.new("ScreenGui", PlayerGui)
    local popupFrame = Instance.new("Frame", popupGui)
    popupFrame.Size = UDim2.fromOffset(350, 150)
    popupFrame.Position = UDim2.fromScale(0.5, 0.5)
    popupFrame.AnchorPoint = Vector2.new(0.5, 0.5)
    popupFrame.BackgroundColor3 = Color3.new(0.1,0.1,0.1)
    Instance.new("UICorner", popupFrame).CornerRadius = UDim.new(0,8)
    Instance.new("UIStroke", popupFrame).Color = Color3.new(0,1,1)
    Instance.new("UIStroke", popupFrame).Thickness = 2

    local titleLbl = Instance.new("TextLabel", popupFrame)
    titleLbl.Size = UDim2.fromScale(0.9, 0.2)
    titleLbl.Position = UDim2.fromScale(0.05, 0.1)
    titleLbl.BackgroundTransparency = 1
    titleLbl.TextColor3 = Color3.new(1,1,1)
    titleLbl.Text = "w è¶…çº§è„šæœ¬"
    titleLbl.TextSize = 22
    titleLbl.Font = Enum.Font.RobotoCondensed

    local contentLbl = Instance.new("TextLabel", popupFrame)
    contentLbl.Size = UDim2.fromScale(0.9, 0.2)
    contentLbl.Position = UDim2.fromScale(0.05, 0.35)
    contentLbl.BackgroundTransparency = 1
    contentLbl.TextColor3 = Color3.new(1,1,1)
    contentLbl.Text = "å½“å‰ç”¨æˆ·: "..LocalPlayer.Name
    contentLbl.TextSize = 16
    contentLbl.Font = Enum.Font.RobotoCondensed

    createButton(popupFrame, "å–æ¶ˆ", UDim2.fromScale(0.1, 0.65), UDim2.fromScale(0.35, 0.2), Color3.new(0.4,0.1,0.1), function()
        popupGui:Destroy()
    end)
    createButton(popupFrame, "åŠ è½½è„šæœ¬", UDim2.fromScale(0.55, 0.65), UDim2.fromScale(0.35, 0.2), Color3.new(0.1,0.4,0.1), function()
        popupGui:Destroy()
        createMainUI()
    end)
end

-- å¯åŠ¨å¼¹çª—
showStartupPopup()
