    end

    -- 5. å†…å®¹å®¹å™¨ï¼ˆå³ä¾§ï¼‰
    local contentContainer = Instance.new("Frame", Global.MainWindow)
    contentContainer.Size = UDim2.fromScale(0.75, 0.92)
