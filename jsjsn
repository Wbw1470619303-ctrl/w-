    createButton(section1, "+", UDim2.fromScale(0.65, 0.45), UDim2.fromScale(0.1, 0.15), Color3.new(0,1,0), function()
        if Global.currentJumpPower < 200 then
            Global.currentJumpPower +=5
            local char = waitForCharacter()
            if char then char:FindFirstChildOfClass("Humanoid").JumpPower = Global.currentJumpPower end
            for _, c in ipairs(section1:GetChildren()) do
                if c:IsA("TextLabel") and c.Text:find("è·³è·ƒé«˜åº¦") then
                    c.Text = "è·³è·ƒé«˜åº¦ï¼ˆå½“å‰ï¼š"..Global.currentJumpPower.."ï¼‰"
                end
            end
        end
    end)

    createParagraph(section1, "çµæ•åº¦ï¼ˆå½“å‰ï¼š"..Global.currentMoveSensitivity.."ï¼‰", UDim2.fromScale(0.05, 0.7), UDim2.fromScale(0.4, 0.1))
    createButton(section1, "-", UDim2.fromScale(0.5, 0.7), UDim2.fromScale(0.1, 0.15), Color3.new(1,0,0), function()
        if Global.currentMoveSensitivity > 0.5 then
            Global.currentMoveSensitivity = math.round((Global.currentMoveSensitivity - 0.1)*10)/10
