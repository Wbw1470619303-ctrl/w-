    -- 7. é€šç”¨åŠŸèƒ½æ ‡ç­¾å†…å®¹
    Global["é€šç”¨åŠŸèƒ½Content"] = Instance.new("Frame", contentContainer)
    Global["é€šç”¨åŠŸèƒ½Content"].Size = UDim2.fromScale(1,1)
    Global["é€šç”¨åŠŸèƒ½Content"].BackgroundTransparency = 1
    Global["é€šç”¨åŠŸèƒ½Content"].Visible = false

    -- åŸºç¡€å±æ€§è°ƒæ•´ï¼ˆåŸç”ŸæŒ‰é’®ï¼‰
    local section1 = createSection(Global["é€šç”¨åŠŸèƒ½Content"], "åŸºç¡€å±æ€§è°ƒæ•´", UDim2.fromScale(0.05, 0.02), UDim2.fromScale(0.9, 0.3))
    createParagraph(section1, "ç§»åŠ¨é€Ÿåº¦ï¼ˆå½“å‰ï¼š"..Global.currentWalkSpeed.."ï¼‰", UDim2.fromScale(0.05, 0.2), UDim2.fromScale(0.4, 0.1))
    createButton(section1, "-", UDim2.fromScale(0.5, 0.2), UDim2.fromScale(0.1, 0.15), Color3.new(1,0,0), function()
