                end
            end
        end
    end)
    createButton(section1, "+", UDim2.fromScale(0.65, 0.2), UDim2.fromScale(0.1, 0.15), Color3.new(0,1,0), function()
        if Global.currentWalkSpeed < 200 then
            Global.currentWalkSpeed +=5
            local char = waitForCharacter()
            if char then char:FindFirstChildOfClass("Humanoid").WalkSpeed = Global.currentWalkSpeed end
            -- æ›´æ–°æ˜¾ç¤º
            for _, c in ipairs(section1:GetChildren()) do
                if c:IsA("TextLabel") and c.Text:find("ç§»åŠ¨é€Ÿåº¦") then
