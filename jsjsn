    Global["ä½œè€…ä¿¡æ¯Content"].Size = UDim2.fromScale(1,1)
    Global["ä½œè€…ä¿¡æ¯Content"].BackgroundTransparency = 1
    createParagraph(Global["ä½œè€…ä¿¡æ¯Content"], "ä½œè€…ï¼šwbwhnb", UDim2.fromScale(0.1, 0.2), UDim2.fromScale(0.8, 0.1))
