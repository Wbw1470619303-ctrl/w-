    createParagraph(Global["ä½œè€…ä¿¡æ¯Content"], "è„šæœ¬æ€§è´¨ï¼šå…è´¹å¼€æº", UDim2.fromScale(0.1, 0.5), UDim2.fromScale(0.8, 0.1))
    createButton(Global["ä½œè€…ä¿¡æ¯Content"], "å¤åˆ¶QQ", UDim2.fromScale(0.1, 0.65), UDim2.fromScale(0.3, 0.1), Color3.new(0,0.8,0), function()
        setclipboard("2318995920")
