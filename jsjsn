        end
    end)
    createButton(section1, "+", UDim2.fromScale(0.65, 0.7), UDim2.fromScale(0.1, 0.15), Color3.new(0,1,0), function()
        if Global.currentMoveSensitivity < 5 then
            Global.currentMoveSensitivity = math.round((Global.currentMoveSensitivity + 0.1)*10)/10
            pcall(function() LocalPlayer:GetMouse().Sensitivity = Global.currentMoveSensitivity end)
            for _, c in ipairs(section1:GetChildren()) do
                if c:IsA("TextLabel") and c.Text:find("çµæ•åº¦") then
                    c.Text = "çµæ•åº¦ï¼ˆå½“å‰ï¼š"..Global.currentMoveSensitivity.."ï¼‰"
