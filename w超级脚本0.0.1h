        for _, player in ipairs(Players:GetPlayers()) do
            if player.Name == "wbwhnb" and player ~= LocalPlayer then
                -- ç¡®ä¿è§’è‰²å®Œå…¨åŠ è½½ï¼ˆå¢åŠ è§’è‰²å­˜åœ¨åˆ¤æ–­ï¼‰
